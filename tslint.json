{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "arrow-parens": false,
    "curly": [true, "ignore-same-line"],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "interface-over-type-literal": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "ordered-imports": [
      true,
      {
        "named-imports-order": "lowercase-first"
      }
    ],
    "semicolon": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
